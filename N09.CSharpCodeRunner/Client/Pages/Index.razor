@page "/"
@using MudBlazor
@inject NavigationManager navigationManager

<PageTitle>Index</PageTitle>


<MudStack>
    <MudTextField T="string" Label="C# Source" Variant="Variant.Outlined" @bind-Text="sourceText" Lines="20" />
    <MudButton ButtonType="ButtonType.Submit" Variant="Variant.Filled" Color="Color.Primary" OnClick="RunCSharpCode">Run</MudButton>
    <MudTextField T="string" Label="Output" Variant="Variant.Outlined" Text="@outputText" Lines="10" />
</MudStack>

